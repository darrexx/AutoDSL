@style("model/AutoDSL.style")
graphModel AutoDSL {
	package info.scce.cinco.product.autoDSL
	nsURI "http://cinco.scce.info/product/autodsl"
	diagramExtension "autodsl"
	
	@style(labeledCircle, "${label}")
	node SomeNode {
		incomingEdges (*)
		outgoingEdges (*)
		attr EString as label
	}	
	
	@style(simpleArrow)
	edge Transition { 
	}
}

